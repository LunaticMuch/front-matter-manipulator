{
  "name": "@lunaticmuch/front-matter-manipulator",
  "version": "2.0.2",
  "type": "module",
  "description": "A utility for parsing and manipulating documents with Front Matter",
  "directories": {
    "lib": "lib"
  },
  "exports": "./lib/index.js",
  "bin": "./lib/cli.cjs",
  "scripts": {
    "test": "jest --runInBand",
    "docs": "npm run docs:cli && npm run docs:scripts",
    "docs:cli": "documentation build ./lib/commands/*.js -f md --shallow -o docs/cli.md --sort-order alpha",
    "docs:scripts": "documentation build ./lib/core/*.js -f md --shallow -o docs/scripts.md --sort-order alpha",
    "lint": "eslint lib/**/*.js",
    "precommit": "npm test",
    "prepush": "npm test"
  },
  "author": "Chris D. Macrae",
  "contributors": [
    {
      "name": "Stefano Cislaghi",
      "url": "https://stefanocislaghi.com"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "chalk": "^4.0.1",
    "deepmerge": "^4.2.2",
    "documentation": "^14.0.0",
    "glob": "^8.0.3",
    "gray-matter": "^4.0.3",
    "lodash": "^4.17.4",
    "yamljs": "^0.3.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/preset-env": "^7.18.10",
    "babel-jest": "^29.0.1",
    "eslint": "^8.23.0",
    "husky": "^8.0.1",
    "jest": "^29.0.1"
  },
  "keywords": [
    "markdown",
    "front matter",
    "frontmatter",
    "editor",
    "manipulating",
    "manipulator",
    "read",
    "write"
  ]
}
